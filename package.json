    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
    "@angular-devkit/build-angular": "^16.2.9",
    "@angular-devkit/core": "^16.2.9",
    "@angular-devkit/schematics": "^16.2.9",
