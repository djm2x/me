{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "f": "set NODE_ENV=production && ng serve --port 4206 --hmr",
    "copy": "node --require ts-node/register ./copy.script.ts",
    "f-p0": "ng build --configuration production --output-path docs --base-href=/me/ && npm run copy",
    "f-p": "ng build --configuration production --output-path docs --base-href=/me && npm run copy",
    "analyze": "ng run portfolio:analyze --diffLoading=false",
    "git": "---------------------------------------------------------------------",
    "create-branch": "git branch new-one",
    "move-to-branch": "git checkout new-one",
    "move-to-master-branch": "git checkout",
    "merge-branch-to-master-branch": "git merge new-one",
    "init": "git init && git remote add origin https://github.com/djm2x/me.git",
    "push": "git add . && git commit -m \"me & you\" && git push -u origin master",
    "ci": "npm run f-p && npm run push && echo App deployed successfuly, open your browser on https://mohamed-mourabit.com",
    "u": "ng update @angular/core@16 @angular/cli@16 --force --allow-dirty && ng update @angular/material@16  --force --allow-dirty",
    "a": "npx npm-check-updates -u",
    "echo": "echo App deployed successfuly, open your browser on https://djm2x.github.io/me/"
  },
  "private": true,
  "dependencies": {
